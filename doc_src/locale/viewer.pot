msgstr ""

#: ../viewer.rst:119
# c59111ee49ba4376b1cdf40b34f552cb
msgid "Индикаторы"
msgstr ""

#: ../viewer.rst:122
# 34275d70aca9402a9e0840dd04f0c9ad
