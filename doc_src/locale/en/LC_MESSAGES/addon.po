msgid "Incomplete mesh; Material settings require vertex colors"
msgstr ""

#: ../addon.rst:110
msgid "Incomplete vehicle. The NAME vehicle doesn't have any chassis or hull"
msgstr ""

#: ../addon.rst:115
msgid "Incomplete vehicle. The NAME vehicle requires at least one bob"
msgstr ""

#: ../addon.rst:119
msgid "Incomplete vehicle. The NAME vehicle requires at least one wheel"
msgstr ""

#: ../addon.rst:128
msgid "Incorrect vertex animation; Object has no vertex animation"
msgstr ""

#: ../addon.rst:131
msgid "Incorrect vertex animation; Unbaked \"ANIM_NAME\" vertex animation"
msgstr ""

#: ../addon.rst:135
msgid "The material has a normal map but doesn't have any material nodes"
msgstr ""

#: ../addon.rst:139
msgid "The mesh has a UV map but has no exported material"
msgstr ""

#: ../addon.rst:143
msgid "The mesh has a vertex color layer but has no exported material"
msgstr ""

#: ../addon.rst:163
msgid ""
"Node material invalid; Check sockets compatibility: FROM_NODE with TO_NODE"
msgstr ""

#: ../addon.rst:172
msgid ""
"Object data not available; Check the \"Do not export\" flag on the "
"OBJECT_NAME data"
msgstr ""

#: ../addon.rst:178
msgid ""
"Object-parent relation is not supported; Clear the parent's inverse "
"transform"
msgstr ""

#: ../addon.rst:187
msgid "Only 2 UV textures are allowed for a mesh; The mesh has N UVs"
msgstr ""

#: ../addon.rst:27
msgid ""
"Blend4Web initialization error! Addon is not compatible with the PLATFORM "
"platform."
msgstr ""

#: ../addon.rst:31
msgid ""
"Blend4Web initialization error! Blender version VER_REQUIRED is required for"
" the Blend4Web addon. The current version is VER_CURRENT."
msgstr ""

#: ../addon.rst:202
msgid ""
"Particle system error; The GROUP_NAME dupli group contains no valid object "
"for export"
msgstr ""

#: ../addon.rst:210
msgid ""
"Particle system error; The \\\"NAME\\\" vertex color specified in the "
"``from`` field is missing in the last of the \\\"OBJECT_NAME\\\" object's "
"vertex colors"
msgstr ""

#: ../addon.rst:216
msgid ""
"Particle system error; The \\\"NAME\\\" vertex color specified in the "
"\"``to`` field is missing in the \\\"OBJECT_NAME\\\" object "
"(\\\"GROUP_NAME\\\" dupli group)"
msgstr ""

#: ../addon.rst:222
msgid ""
"Particle system error; The \\\"NAME\\\" vertex color specified in the ``to``"
" field is missing in the list of the \\\"OBJECT_NAME\\\" object's vertex "
"colors"
msgstr ""

