msgstr ""

#: ../particles.rst:199
# 0f818284ad0448e293f438e0a011799b
msgid "Текстуры материала частиц"
msgstr ""

#: ../particles.rst:201
# eea7cc7c5d324b2881151e9e92f70c8f
